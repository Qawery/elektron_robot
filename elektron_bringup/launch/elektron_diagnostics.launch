<launch>	
	<!-- Laptop battery monitor -->
	<node name="laptop_battery" pkg="elektron_base" type="laptop_battery.py"/>
	
	<node name="wifi_monitor" pkg="elektron_base" type="wifi_monitor.py"/>
	
	<!-- Prepare diagnostic_aggregator and load it's analyzers -->
	<node pkg="diagnostic_aggregator" type="aggregator_node" name="diagnostic_aggregator" >
	<rosparam command="load" file="$(find elektron_base)/config/diagnostics.yaml" />
	</node>
</launch>